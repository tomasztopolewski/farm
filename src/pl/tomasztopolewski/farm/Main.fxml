            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Label fx:id="labelFooter" alignment="CENTER_RIGHT" layoutX="16.0" layoutY="666.0" prefHeight="17.0" prefWidth="965.0" text="Farm version type-version by author " textAlignment="RIGHT" textFill="#a4a4a4" />
